<li class="field-group course-advanced-policy-list-item">
	<div class="field is-not-editable text key">
		<h3 class="title" id="<%= keyUniqueId %>"><%= item.display_name %></h3>
  </div>

  <div class="field text value">
    <label class="sr" for="<%= valueUniqueId %>">Value:</label>
    <textarea class="json text" id="<%= valueUniqueId %>"><%= value %></textarea>
    <span class="tip tip-stacked"><%= item.help %></span>
  </div>
</li>
